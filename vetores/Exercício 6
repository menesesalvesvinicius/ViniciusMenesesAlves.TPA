import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      
      int max = 11;
      int[] a = new int[max];
      
      for (int i = 0; i < max; i++) {
        int pow = 2;
        for (int i2 = 1; i2 < i; i2++) {
          pow *= 2;
        };
        if (i == 0) {
          pow = 0;
        };
        
        System.out.println(pow);
      };
  }
}
