import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      Scanner l = new Scanner(System.in);
      
      int max = 10;
      int[] a = new int[max];
      
      for (int i = 0; i < max; i++) {
        a[i] = l.nextInt();
      };
      
      for (int i = 0; i < max; i++) {
        double Origin = a[i];
        boolean Pon = true;
        for (int IndexPrimeornot = a[i] - 1; IndexPrimeornot > 1; IndexPrimeornot--) {
          if (Origin / IndexPrimeornot == Math.floor(Origin / IndexPrimeornot)) {
            Pon = false;
          };
        };
        if (Origin == 1) {
            Pon = false;
        };
        if (Pon == true) {
          System.out.println(Origin + " É um número primo");
        }else {
          System.out.println(Origin + " Não é um número primo");
        };
      };
  }
}
